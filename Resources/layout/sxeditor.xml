            <KPushButton objectName="alignright" fixWidth="24" iconSize="24" iconPath="res:/Resources/image/editico_alignright.png"/>
			<KPushButton objectName="textcolor" fixWidth="24" iconSize="24" iconPath="res:/Resources/image/editico_textcolor.png"/>
		  </KToolBar>
