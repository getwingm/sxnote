            <KPushButton objectName="save" fixWidth="24" iconSize="24" iconPath="res:/Resources/image/editico_save.png"/>
            <KPushButton objectName="copy" fixWidth="24" iconSize="24" iconPath="res:/Resources/image/editico_copy.png"/>
            <KPushButton objectName="paste" fixWidth="24" iconSize="24" iconPath="res:/Resources/image/editico_paste.png"/>
            <KPushButton objectName="cut" fixWidth="24" iconSize="24" iconPath="res:/Resources/image/editico_cut.png"/>
            <KPushButton objectName="link" fixWidth="24" iconSize="24" iconPath="res:/Resources/image/editico_link.png"/>
            <KPushButton objectName="replace" fixWidth="24" iconSize="24" visible="true" iconPath="res:/Resources/image/editico_replace.png"/>
            <KPushButton objectName="find" fixWidth="24" iconSize="24" visible="true" iconPath="res:/Resources/image/editico_find.png"/>
